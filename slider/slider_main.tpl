
<div id="main-carousel" class="carousel slide" data-ride="carousel">
    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
        @imageSliderContent@
    </div>

    <!-- Controls -->
    <a class="left carousel-control iconz-arrow-left feather" href="#main-carousel" role="button" data-slide="prev">
    </a>
    <a class="right carousel-control iconz-arrow-right feather" href="#main-carousel" role="button" data-slide="next">
    </a>
</div>